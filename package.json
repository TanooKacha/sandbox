    "@nestjs/core": "^6.10.14",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/mongoose": "^6.3.1",
    "@nestjs/passport": "^6.1.1",
    "@nestjs/platform-express": "^6.10.14",
    "@nestjs/typeorm": "^6.2.0",
    "@types/mongoose": "^5.7.1",
    "@types/passport-jwt": "^3.0.3",
    "axios": "^0.19.2",
    "bull": "^3.13.0",
    "crypto-js": "^4.0.0",
